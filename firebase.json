{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    },
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/auth/**",
        "function": "authFunction"
      },
      {
        "source": "**",
        "function": "ssrmysite"
      }
    ]
  }
}
